<settings xmlns="http://maven.apache.org/SETTINGS/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>splicemachine</id>
      <username>REPLACE_USER</username>
      <password>REPLACE_PASS</password>
    </server>
    <server>
      <id>splicemachine-public</id>
      <username>REPLACE_USER</username>
      <password>REPLACE_PASS</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>splicemachine</id>
          <url>http://nexus.splicemachine.com/nexus/content/groups/developers</url>
          <snapshots>
            <updatePolicy>always</updatePolicy>
          </snapshots>
          <releases>
            <updatePolicy>always</updatePolicy>
          </releases>
        </repository>
        <repository>
          <id>splicemachine-public</id>
          <url>http://repository.splicemachine.com/nexus/content/groups/public</url>
          <snapshots>
            <updatePolicy>always</updatePolicy>
          </snapshots>
          <releases>
            <updatePolicy>always</updatePolicy>
          </releases>
        </repository>
      </repositories>
    </profile>
  </profiles>
</settings>